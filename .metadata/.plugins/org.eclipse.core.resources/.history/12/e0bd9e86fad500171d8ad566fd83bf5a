
public class UsingVariables 
{
	public static void main( String[] args)
	{
		int room_number, Class;
		double e;
		
		room_number = 113;
		Class = compsci;
		e = 2.71828;
		
		System.out.println("This is room # " + room_number +);
		System.out.println("e is clos to " + e +);
		System.out.println("I am learning a bit about " + Class +);
		

	}
}
