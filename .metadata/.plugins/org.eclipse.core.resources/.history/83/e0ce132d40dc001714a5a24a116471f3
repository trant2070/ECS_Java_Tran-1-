import java.util.Random;
import java.util.Scanner;

public class DoubleDice
{
	public static void main ( String[] args )
	{
		Random r = new Random();
		String again;
		int x =  1 + r.nextInt(6);
		int y = 1 + r.nextInt(6);
		
		while(x != y)
		{
			
			
			System.out.println("Roll #1: " + x );
			System.out.println("Roll #2: " + y);
			
			System.out.print("the total is: " + (x+y));
			
		}
		
		
		
	}
}