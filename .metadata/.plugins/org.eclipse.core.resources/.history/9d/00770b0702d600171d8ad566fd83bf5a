
public class YourSchedule 
{
	public static void main( String[] args)
	{
		String myFirst, mySecond, myThird, myFourth, teach1, teach2, teach3, teach4;
		int myPer1, myPer2, myPer3, myPer4; 
		
		myFirst= "Modern Epic";
		mySecond="ECS";
		myThird= "Physics";
		myFourth= "APUSH";
		myPer1= 1;
		myPer2= 2;
		myPer3= 3;
		myPer4= 4;
		teach1= "Leal";
		teach2= "Muaro";
		teach3= "Barrows";
		teach4= "Vanover";
				
		
		
		System.out.println("Tyler Tran's Class Schedule");
		System.out.println("|Per " + myPer1 + " |		" + myFirst + "		|		" + teach1 + "		|" );
		System.out.println("|Per " + myPer2 + " |		" + mySecond + "			|		" + teach2 + "		|" );
		System.out.println("|Per " + myPer3 + " |		" + myThird + "			|		" + teach3 + "		|" );
		System.out.println("|Per " + myPer4 + " |		" + myFourth + "			|		" + teach4 + "		|" );
	}
	
}
