		int myPer1, myPer2, myPer3, myPer4;
		
		myName= "Tyler Tran";
		myFirst= "Modern Epic";
		mySecond="ECS";
		myThird= "Physics";
		myFourth= "APUSH";
		
		
		System.out.println(+ myName + "'s Class Schedule");
		System.out.println("|Per 1  |		" + myFirst + "		|		Leal		|" );
