import java.util.Random;
 
public class Dice
{
	public static void main ( String[] args )
	{
		Random r = new Random();
		int x =  r.nextInt(6);
		int y = r.nextInt(6);

		
		System.out.println("Roll #1: " + x);
		System.out.println("Roll #2: " + y);
		System.out.println("The total is " + (x+y));
		
		
		
	}
}